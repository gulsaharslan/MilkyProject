@model List<MilkyProject.WebUI.Dtos.GalleryDto.ResultGalleryDto>

<div class="container-xxl py-5 px-0">
    <div class="row g-0">
        @foreach(var item in Model)
        {
        <div class="col-lg-3 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
            <div class="row g-0">
                <div class="col-12">
                        <a class="d-block" href="@Url.Content("~/upload/" + item.imageUrl)" data-lightbox="gallery">
                            <img class="img-fluid" src="@Url.Content("~/upload/" + item.imageUrl)" alt="">
                    </a>
                </div>
            </div>
        </div>
        }
    </div>
</div>