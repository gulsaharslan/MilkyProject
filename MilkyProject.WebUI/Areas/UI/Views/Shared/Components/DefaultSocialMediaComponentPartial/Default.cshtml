@model List<MilkyProject.WebUI.Dtos.SocialMediaDto.ResultSocialMediaDto>

<div class="d-flex justify-content-center">
    @foreach(var item in Model)
    {
        <a class="btn btn-square btn-outline-secondary rounded-circle mx-1" href="@Url.Content(item.url)" target="_blank"><i class="@item.icon"></i></a>
    }
</div>