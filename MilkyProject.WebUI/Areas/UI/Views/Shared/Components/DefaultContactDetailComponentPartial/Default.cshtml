@model List<MilkyProject.WebUI.Dtos.ContactDetailDto.ResultContactDetailDto>

    @foreach (var item in Model)
    {
        <div class="col-lg-6 wow fadeInUp" data-wow-delay="0.5s">
            <h3 class="mb-4">İletişim</h3>
        <div class="d-flex border-bottom pb-3 mb-3">
            <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
                <i class="fa fa-map-marker-alt text-body"></i>
            </div>
            <div class="ms-3">
                <h6>Ofisimiz</h6>
                <span>@item.adress</span>
            </div>
        </div>
        <div class="d-flex border-bottom pb-3 mb-3">
            <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
                <i class="fa fa-phone-alt text-body"></i>
            </div>
            <div class="ms-3">
                <h6>Telefon</h6>
                <span>@item.phone</span>
            </div>
        </div>
        <div class="d-flex border-bottom-0 pb-3 mb-3">
            <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
                <i class="fa fa-envelope text-body"></i>
            </div>
            <div class="ms-3">
                <h6>E-Mail</h6>
                <span>@item.email</span>
            </div>
        </div>
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d96326.54476911004!2d28.692945123689622!3d41.02078117824076!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14caa4102814e62d%3A0xf17358ce73944614!2zS8O8w6fDvGvDp2VrbWVjZS_EsHN0YW5idWw!5e0!3m2!1str!2str!4v1717704571742!5m2!1str!2str" frameborder="0" style="min-height: 300px; width:400px; border:0;" allowfullscreen="" aria-hidden="false"
                tabindex="0"></iframe>
     
    </div>
    }
</div>
