
<div class="col-md-6 grid-margin stretch-card">
    <div class="card tale-bg">
        <div class="card-people mt-auto">
            <img src="~/skydashadmin/images/dashboard/people.svg" style="height:200px" alt="people">
            <div class="weather-info">
                <div class="d-flex">
                    <div class="ml-2" style="margin-right:200px">
                        <h4 class="location font-weight-normal">@ViewBag.status</h4>
                    </div>
                    <div>
                        <h2 class="mb-0 font-weight-normal">@ViewBag.temperature<sup>C</sup></h2>
                    </div>
                    <div class="ml-2">
                        <h4 class="location font-weight-normal">@ViewBag.city</h4>
                        <h6 class="font-weight-normal">Turkey</h6>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</div>